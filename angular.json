{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "top-nav-lib": {
      "projectType": "library",
      "root": "projects/top-nav-lib",
      "sourceRoot": "projects/top-nav-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/top-nav-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/top-nav-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/top-nav-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/top-nav-lib/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"]
          }
        }
      }
    },
    "menu-bar-lib": {
      "projectType": "library",
      "root": "projects/menu-bar-lib",
      "sourceRoot": "projects/menu-bar-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/menu-bar-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/menu-bar-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/menu-bar-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/menu-bar-lib/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"]
          }
        }
      }
    },
    "tab-management-lib": {
      "projectType": "library",
      "root": "projects/tab-management-lib",
      "sourceRoot": "projects/tab-management-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/tab-management-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/tab-management-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/tab-management-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/tab-management-lib/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"]
          }
        }
      }
    },
    "shell-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/shell-app",
      "sourceRoot": "apps/shell-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/shell-app",
            "index": "apps/shell-app/src/index.html",
            "browser": "apps/shell-app/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "apps/shell-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "apps/shell-app/public"
              }
            ],
            "styles": [
              "apps/shell-app/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "shell-app:build:production"
            },
            "development": {
              "buildTarget": "shell-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "apps/shell-app/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "apps/shell-app/public"
              }
            ],
            "styles": [
              "apps/shell-app/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
